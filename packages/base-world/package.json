{
  "name": "@tenetxyz/base-world",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "scripts": {
    "mud": "node ../../node_modules/@latticexyz/cli/dist/mud.js",
    "build": "forge build && yarn run build:js",
    "build:js": "tsup",
    "clean": "forge clean",
    "build:local": "yarn run initialize",
    "deploy:local": "yarn initialize && yarn mud deploy",
    "deploy": "yarn initialize && yarn mud deploy",
    "deploy:hackathon": "yarn initialize && yarn mud deploy --profile=hackathon-testnet",
    "deploy:testnet": "yarn initialize && yarn mud deploy --profile=lattice-testnet",
    "dev": "yarn mud dev-contracts --tsgenOutput ../client/src/mud",
    "hotreload": "sh hotreload.sh",
    "initialize": "yarn tablegen && yarn worldgen && yarn customScripts && yarn build && yarn worldtypes",
    "customScripts": "../../scripts/replaceGeneratedTypes.sh && python3 script/replaceImports.py src/codegen/world/IWorld.sol",
    "lint": "yarn prettier && yarn solhint",
    "prettier": "prettier --write 'src/**/*.sol'",
    "solhint": "solhint --config ./.solhint.json 'src/**/*.sol' --fix",
    "tablegen": "yarn mud tablegen",
    "test": "yarn mud test",
    "test-verbose": "yarn initialize && yarn mud test --forgeOptions='-vvv'",
    "worldgen": "yarn mud worldgen",
    "worldtypes": "rimraf types && typechain --target=ethers-v5 out/world/IWorld.sol/IWorld.json"
  },
  "dependencies": {
    "@tenetxyz/base-ca": "workspace:*",
    "@tenetxyz/registry": "workspace:*",
    "@tenetxyz/utils": "workspace:*"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@latticexyz/cli": "link:../../../mud/packages/cli",
    "@latticexyz/config": "link:../../../mud/packages/config",
    "@latticexyz/noise": "link:../../../mud/packages/noise",
    "@latticexyz/schema-type": "link:../../../mud/packages/schema-type",
    "@latticexyz/solecs": "link:../../../mud/packages/solecs",
    "@latticexyz/std-contracts": "link:../../../mud/packages/std-contracts",
    "@latticexyz/store": "link:../../../mud/packages/store",
    "@latticexyz/world": "link:../../../mud/packages/world",
    "@openzeppelin/contracts": "^4.9.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/node": "^18.15.11",
    "abdk-libraries-solidity": "^3.2.0",
    "ds-test": "https://github.com/dapphub/ds-test.git",
    "ethers": "^5.7.2",
    "forge-std": "https://github.com/foundry-rs/forge-std",
    "memmove": "https://github.com/dk1a/memmove.git",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "rimraf": "^5.0.1",
    "solhint": "^3.3.7",
    "solhint-config-mud": "2.0.0-alpha.1.197+5e6cc0f5",
    "solhint-plugin-mud": "2.0.0-alpha.1.197+5e6cc0f5",
    "tsup": "^7.2.0",
    "typechain": "^8.1.1"
  }
}
