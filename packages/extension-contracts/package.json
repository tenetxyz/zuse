{
  "name": "extension-contracts",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "mud": "node ../../node_modules/@latticexyz/cli/dist/mud.js",
    "build": "forge clean && forge build",
    "deploy": "yarn run initialize && yarn mud deploy",
    "deploy-local": "yarn run initialize && yarn mud deploy --installDefaultModules false --worldAddress 0x5FbDB2315678afecb367f032d93F642f64180aa3",
    "deploy:hackathon": "yarn run initialize && yarn mud deploy --profile=hackathon-testnet",
    "deploy:testnet": "yarn run initialize && yarn mud deploy --profile=lattice-testnet",
    "dev": "yarn mud dev-contracts --tsgenOutput ./client",
    "replaceGeneratedTypes": "./script/replaceGeneratedTypes.sh",
    "initialize": "yarn run tablegen && yarn run worldgen && yarn run replaceGeneratedTypes && yarn run build && yarn run worldtypes && yarn run tsgen",
    "lint": "yarn run prettier && yarn run solhint",
    "prettier": "prettier --write 'src/**/*.sol'",
    "solhint": "solhint --config ./.solhint.json 'src/**/*.sol' --fix",
    "tablegen": "yarn mud tablegen",
    "test": "yarn mud test",
    "test-verbose": "yarn run initialize && yarn mud test --forgeOptions='-vvv'",
    "tsgen": "yarn mud tsgen --configPath mud.config.ts --out ./client",
    "worldgen": "yarn mud worldgen",
    "worldtypes": "rimraf types && typechain --target=ethers-v5 out/world/IWorld.sol/IWorld.json"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@latticexyz/cli": "link:../../../mud/packages/cli",
    "@latticexyz/config": "link:../../../mud/packages/config",
    "@latticexyz/schema-type": "link:../../../mud/packages/schema-type",
    "@latticexyz/std-contracts": "link:../../../mud/packages/std-contracts",
    "@latticexyz/store": "link:../../../mud/packages/store",
    "@latticexyz/world": "link:../../../mud/packages/world",
    "@tenetxyz/contracts": "workspace:*",
    "ethers": "^5.7.2",
    "rimraf": "^5.0.1"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^4.9.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/node": "^18.15.11",
    "ds-test": "https://github.com/dapphub/ds-test.git#c9ce3f25bde29fc5eb9901842bf02850dfd2d084",
    "forge-std": "https://github.com/foundry-rs/forge-std",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7",
    "solhint-config-mud": "2.0.0-alpha.1.197+5e6cc0f5",
    "solhint-plugin-mud": "2.0.0-alpha.1.197+5e6cc0f5",
    "typechain": "^8.1.1"
  }
}
